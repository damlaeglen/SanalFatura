        $("form button.btn").on("click", function () {

            let mail = {
                "Ad": $("input#Ad").val(),
                "Telefon": $("input#Telefon").val(),
                "Mail": $("input#Mail").val(),
                "Konu": $("input#Konu").val(),
                "Mesaj": $("textarea#Mesaj").val(),
            };
            toastr.options = {
                "closeButton": false,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-bottom-right",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }
            toastr.success("Başarıyla mail gönderdiniz.")
            $.ajax({
                url: "@Url.Action("index","Efatura")",
                type: "post",
                dataType: "JSON",
                data: mail,
                success: function (res) {
                    console.log(res)
                    if (res.IsSuccess) {

                    } else { }
                    for (var i = 0; i < 5; i++) {
                        $("form")[i].reset()
                    }

                },
                error: function () {

                }
            });

            return false;
        })
    </script>
